unit EmployeesReferenceRecordViewModel;

interface

uses

  ReferenceFormRecordViewModel,
  SysUtils,
  Classes;

type

  TEmployeesReferenceRecordViewModel = class

    public

      Id: Variant;
      Name: String;
      Surname: String;
      Patronymic: String;
      BirthDate: TDateTime;
      Speciality: String;

      CanBeChanged: Variant;
      CanBeRemoved: Variant;

      constructor Create;
      
  end;

implementation

uses

  Variants;
  
{ TEmployeesReferenceRecordViewModel }

constructor TEmployeesReferenceRecordViewModel.Create;
begin

  inherited;

  CanBeChanged := Null;
  CanBeRemoved := Null;
  
end;

end.
