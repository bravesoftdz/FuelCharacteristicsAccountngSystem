unit FuelReservoirCalibrationChart;

interface

uses

  DomainObjectValueUnit,
  DomainException;

type

  TFuelReservoirCalibrationChartException = class (TDomainException)

  end;

  TReservoirCalibrationChartInput = class (TDomainObjectValue)

    private

      FRiseLevel: Single;
      FTemperature: Single;
      FDensity: Single;

    public

      property RiseLevel: Single read FRiseLevel write FRiseLevel;
      property Temperature: Single read FTemperature write FTemperature;
      property Density: Single read FDensity write FDensity;

  end;
  
  IFuelReservoirCalibrationChart = interface

    
  end;

implementation

end.
